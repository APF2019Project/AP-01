@startuml

Class DNA {
	-Image image
	-int speed
	-int powerOfDestruction
	-Location firstLocation
}

DNA <|-- AmmunitionDNA
Class AmmunitionDNA {
	-int cooldown
	-int staticTurnNumber
	-int reduceSpeedRatio
	-int minimumDistanceForShoot
	-boolean crossingShield
}
Ammunition o-- AmmunitionDNA

DNA <|-- BeingDNA
Class BeingDNA {
	-int health
	-int gameCost
	-int shopCost
}
Being o-- BeingDNA

BeingDNA <|-- PlantDNA
Class PlantDNA {
	-int sunIncome
}

BeingDNA <|-- ZombieDNA
Class ZombieDNA {
	
}

Class Ammunition {
	-AmmunitionDNA dna
}

Class Being {
	-BeingDNA dna
}

Being <|-- Plant
Class Plant {
	
}

Being <|-- Zombie
Class Zombie {
	
}

@enduml
